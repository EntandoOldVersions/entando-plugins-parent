<shortcut-def>
	<shortcuts>
		<shortcut id="jpuserprofile.profile.current.entry" requiredPermission="superuser" menusection="plugins" source="jpuserprofile">
			<description key="jpuserprofile.shortcut.profile.current.entry.shortDesc" >Your Profile</description>
			<longdesc key="jpuserprofile.shortcut.profile.current.entry.longDesc">
				<![CDATA[Edit your profile]]>
			</longdesc>
			<url>
				<param name="namespace">/do/jpuserprofile/MyProfile</param>
				<param name="actionName">edit</param>
			</url>
		</shortcut>
		<shortcut id="jpuserprofile.profile.list" requiredPermission="superuser" menusection="plugins" source="jpuserprofile">
			<description key="jpuserprofile.shortcut.profile.list.shortDesc" >Profiles</description>
			<longdesc key="jpuserprofile.shortcut.profile.list.longDesc">
				<![CDATA[Manage the profile list: can view and edit]]>
			</longdesc>
			<url>
				<param name="namespace">/do/jpuserprofile/Search</param>
				<param name="actionName">list</param>
			</url>
		</shortcut>
	</shortcuts>
</shortcut-def>