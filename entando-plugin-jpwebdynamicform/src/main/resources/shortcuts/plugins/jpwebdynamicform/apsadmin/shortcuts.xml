<shortcut-def>
	<shortcuts>
		<!-- admin: message list -->
		<shortcut id="jpwebdynamicform.message.operator" requiredPermission="superuser" menusection="plugins" source="jpwebdynamicform">
			<description key="jpwebdynamicform.message.operator.shortDesc">DWF Messages</description>
			<longdesc key="jpwebdynamicform.message.operator.longDesc">
				<![CDATA[Dynamic Web Forms - Message Management]]>
			</longdesc>
			<url>
				<param name="namespace">/do/jpwebdynamicform/Message/Operator</param>
				<param name="actionName">list</param>
			</url>
		</shortcut>
		<!-- admin: configuration -->
		<shortcut id="jpwebdynamicform.message.config" requiredPermission="superuser" menusection="plugins" source="jpwebdynamicform">
			<description key="jpwebdynamicform.message.config.shortDesc">DWF Config</description>
			<longdesc key="jpwebdynamicform.message.config.longDesc">
				<![CDATA[Dynamic Web Forms Configuration]]>
			</longdesc>
			<url>
				<param name="namespace">/do/jpwebdynamicform/Message/Config</param>
				<param name="actionName">list</param>
			</url>
		</shortcut>
		<!-- admin: entities -->
		<shortcut id="jpwebdynamicform.entity.config" requiredPermission="superuser" menusection="plugins" source="jpwebdynamicform">
			<description key="jpwebdynamicform.entity.config.shortDesc">DWF Message Types</description>
			<longdesc key="jpwebdynamicform.entity.config.longDesc">
				<![CDATA[Dynamic Web Forms Entity Management]]>
			</longdesc>
			<url>
				<param name="namespace">/do/Entity</param>
				<param name="actionName">initViewEntityTypes</param>
				<param name="entityManagerName">jpwebdynamicformMessageManager</param>
			</url>
		</shortcut>
		<!-- operator: message list -->
		<shortcut id="jpwebdynamicform.message.operator" requiredPermission="jpwebdynamicformOperator" menusection="plugins" source="jpwebdynamicform">
			<description key="jpwebdynamicform.message.operator.shortDesc">DWF Messages</description>
			<longdesc key="jpwebdynamicform.message.operator.longDesc">
				<![CDATA[Dynamic Web Forms - Message Management]]>
			</longdesc>
			<url>
				<param name="namespace">/do/jpwebdynamicform/Message/Operator</param>
				<param name="actionName">list</param>
			</url>
		</shortcut>
		
	</shortcuts>
</shortcut-def>