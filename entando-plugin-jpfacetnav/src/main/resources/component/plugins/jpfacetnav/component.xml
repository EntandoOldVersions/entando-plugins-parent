<?xml version="1.0" encoding="UTF-8"?>
<component>
	<code>jpfacetnav</code>
	<description>Faceted Navigation</description>
	<dependencies>
		<code>jacms</code>
	</dependencies>
	<installation>
		<environment code="production">
			<defaultSqlResources>
				<datasource name="portDataSource">
classpath:sql/plugins/jpfacetnav/port_data_production.sql
				</datasource>
				<datasource name="servDataSource">
classpath:sql/plugins/jpfacetnav/serv_data_production.sql
				</datasource>
			</defaultSqlResources>
		</environment>
		<environment code="test">
			<defaultSqlResources>
				<datasource name="portDataSource">
classpath:sql/plugins/jpfacetnav/port_data_test.sql
				</datasource>
				<datasource name="servDataSource">
classpath:sql/plugins/jpfacetnav/serv_data_test.sql
				</datasource>
			</defaultSqlResources>
		</environment>
	</installation>
</component>
